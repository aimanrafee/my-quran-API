{
    "verses": [
        {
            "verse_number": 1,
            "text_uthmani": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Dengan nama Allah, Yang Maha Pemurah, lagi Maha Mengasihani."
                }
            ]
        },
        {
            "verse_number": 2,
            "text_uthmani": "ٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَـٰلَمِينَ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Segala puji tertentu bagi Allah, Tuhan yang memelihara dan mentadbirkan sekalian alam."
                }
            ]
        },
        {
            "verse_number": 3,
            "text_uthmani": "ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Yang Maha Pemurah, lagi Maha Mengasihani."
                }
            ]
        },
        {
            "verse_number": 4,
            "text_uthmani": "مَـٰلِكِ يَوْمِ ٱلدِّينِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Yang Menguasai pemerintahan hari Pembalasan (hari Akhirat)."
                }
            ]
        },
        {
            "verse_number": 5,
            "text_uthmani": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Engkaulah sahaja (Ya Allah) Yang Kami sembah, dan kepada Engkaulah sahaja kami memohon pertolongan."
                }
            ]
        },
        {
            "verse_number": 6,
            "text_uthmani": "ٱهْدِنَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Tunjukilah kami jalan yang lurus."
                }
            ]
        },
        {
            "verse_number": 7,
            "text_uthmani": "صِرَٰطَ ٱلَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ ٱلْمَغْضُوبِ عَلَيْهِمْ وَلَا ٱلضَّآلِّينَ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Iaitu jalan orang-orang yang Engkau telah kurniakan nikmat kepada mereka, bukan (jalan) orang-orang yang Engkau telah murkai, dan bukan pula (jalan) orang-orang yang sesat."
                }
            ]
        }
    ]
}