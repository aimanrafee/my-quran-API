{
    "verses": [
        {
            "verse_number": 1,
            "text_uthmani": " تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Binasalah kedua-dua tangan Abu lahab, dan binasalah ia bersama!"
                }
            ]
        },
        {
            "verse_number": 2,
            "text_uthmani": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Hartanya dan segala yang diusahakannya, tidak dapat menolongnya."
                }
            ]
        },
        {
            "verse_number": 3,
            "text_uthmani": "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Ia akan menderita bakaran api neraka yang marak menjulang."
                }
            ]
        },
        {
            "verse_number": 4,
            "text_uthmani": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Dan juga isterinya, seorang perempuan pemunggah kayu api. -"
                }
            ]
        },
        {
            "verse_number": 5,
            "text_uthmani": "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Di lehernya sejenis tali, dari tali-tali yang dipintal."
                }
            ]
        }
    ]
}