{
    "verses": [
        {
            "verse_number": 1,
            "text_uthmani": " وَٱلْعَصْرِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Demi Masa!"
                }
            ]
        },
        {
            "verse_number": 2,
            "text_uthmani": "إِنَّ ٱلْإِنسَـٰنَ لَفِى خُسْرٍ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Sesungguhnya manusia itu dalam kerugian -"
                }
            ]
        },
        {
            "verse_number": 3,
            "text_uthmani": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ وَتَوَاصَوْا۟ بِٱلْحَقِّ وَتَوَاصَوْا۟ بِٱلصَّبْرِ",
            "translations": [
                {
                    "resource_id": 39,
                    "text": "Kecuali orang-orang yang beriman dan beramal soleh, dan mereka pula berpesan-pesan dengan kebenaran serta berpesan-pesan dengan sabar."
                }
            ]
        }
    ]
}